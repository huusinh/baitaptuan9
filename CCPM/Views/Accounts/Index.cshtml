@model IEnumerable<CCPM.Models.Account>


<table class="table table-hover table-light">
    <thead class="table-dark">
        <tr class="text-center">
            <th>Account</th>
            <th>usename</th>
            <th>password</th>
            <th>status</th>

        </tr>
    </thead>
    <tbody>
        @foreach (Account p in Model)
        {
        <tr>
            <td class="font-weight-bold">
                @p.AccountId
            </td>
            <td>
                @p.Username
            </td>
            <td>
                @p.Password
            </td>
            <td>
                @p.Status
            </td>

            <td>
                <button class="text-primary">Xóa</button>
                <button class="text-danger">Sửa</button>
            </td>
        </tr>
        }
    </tbody>
</table>


